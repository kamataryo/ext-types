{
  "name": "@kamataryo/extypes",
  "version": "0.0.7",
  "author": "kamataryo <kamataryo@users.noreply.github.com>",
  "license": "MIT",
  "description": "JavaScript extension property/method for native types.",
  "main": "./index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./",
    "lint": "eslint . --ext=jsx --ext=js",
    "tokengen": "travis encrypt $([[ $(cat $HOME/.npmrc) =~ _authToken=(.*)$ ]] && echo ${BASH_REMATCH[1]}) --add deploy.api_key",
    "test": "NODE_PATH=./src ava",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postversion": "git push origin master && git push origin $(git tag -l | tail -n1)"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamataryo/extypes.git"
  },
  "keywords": [
    "javascript",
    "prototype",
    "extend"
  ],
  "bugs": {
    "url": "https://github.com/kamataryo/extypes/issues"
  },
  "homepage": "https://github.com/kamataryo/extypes#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "codecov": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.2.0",
    "nyc": "^10.3.2",
    "typescript": "^2.3.3",
    "typescript-eslint-parser": "^3.0.0"
  }
}
