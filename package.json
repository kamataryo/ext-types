{
  "name": "@kamataryo/ext-types",
  "version": "0.0.0",
  "author": "kamataryo <kamataryo@users.noreply.github.com>",
  "license": "MIT",
  "description": "This is toolbox to extend JavaScript built-in Types",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./dist --source-maps inline",
    "lint": "eslint . --ext=jsx --ext=js",
    "tokengen": "travis encrypt $([[ $(cat $HOME/.npmrc) =~ _authToken=(.*)$ ]] && echo ${BASH_REMATCH[1]}) --add deploy.api_key",
    "test": "NODE_PATH=./src ava"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamataryo/ext-types.git"
  },
  "keywords": [
    "javascript",
    "prototype",
    "extend"
  ],
  "bugs": {
    "url": "https://github.com/kamataryo/ext-types/issues"
  },
  "homepage": "https://github.com/kamataryo/ext-types#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.2.0"
  }
}
